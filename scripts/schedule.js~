var schedule = require('node-schedule');
var batch = require('batchNotifier.js');
var config = require('config.js');

var morning = schedule.scheduleJob('5 * * * * *', function(){
    //Execute Daily Batch With Morning Parameters
    console.log('Hello');
    batch.run("../resources/alerts.csv");
});

var evening = schedule.scheduleJob('0 30 18 * * *', function(){
    //Execute Daily Batch With Missed Court Date Parameters
    batch.run("../resources/missed.csv");
});
